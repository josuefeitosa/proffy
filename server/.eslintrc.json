{
    "env": {
        "es2017": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "ts": "never"
        }
      ],
      "no-console": "off",
      "radix": "off",
      "camelcase": "off",
      "class-methods-use-this": "off",
      "func-names":"off"
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
}
